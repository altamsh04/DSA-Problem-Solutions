#include <bits/stdc++.h>
using namespace std;

int main() {
    int T, N;
    cin >> T;
    
    while (T--) {
        cin >> N;
        if (N<4) {
            cout << "0" <<endl;
        }
        else {
            int S = N / 2;
            int L = S / 2;
            int B = S - L;
            cout << L * B <<endl;
        }
    }
}
